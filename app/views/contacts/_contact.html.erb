<div class="thumbnail">
<div class="col-sm-8">
  <p><strong>Name:</strong><%= contact.Title %><%= contact.FirstName%> <%= contact.LastName %></p>
  <p><strong>Cell Phone:</strong><%= contact.MobilePhone%></p>
  <p><strong>Email:</strong><%=contact.EmailName%></p>
</div>
<div class="col-sm-8 rightThumbnail">
  <strong>Address:</strong>
  <p><%= contact.Address%></p>
  <p><%= contact.Address1 %></p>
  <p><%= contact.City%> <%=contact.StateOrProvince %><%=contact.PostalCode %></p>
  <strong>phone:</strong>
  
  <p><%= number_to_phone(contact.WorkPhone, area_code: true) %></p>
  <strong>fax:</strong>
  <p><%= number_to_phone(contact.FaxNumber, area_code: true) %></p>
</div>
  

</div>
